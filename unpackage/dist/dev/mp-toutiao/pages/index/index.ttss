.index swiper {
  width: 750rpx;
  height: 500rpx;
}
.index swiper image {
  height: 100%;
  width: 100%;
}
.index .attraction .title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin: 80rpx 25rpx 30rpx 25rpx;
}
.index .attraction .title .title-left {
  font-size: 32rpx;
  line-height: 32rpx;
  color: #EEEEEE;
}
.index .attraction .title .title-right {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 24rpx;
  line-height: 40rpx;
  color: #8B8C91;
}
.index .attraction .info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  white-space: nowrap;
  width: 750rpx;
  margin-left: 20rpx;
}
.index .attraction .info .wrapper {
  margin: 0 8rpx;
  display: inline-block;
  width: 340rpx;
  height: 240rpx;
  background-color: #24262F;
  border-radius: 15px;
}
.index .attraction .info .wrapper .pic {
  position: relative;
}
.index .attraction .info .wrapper .pic image {
  width: 340rpx;
  height: 170rpx;
  border-radius: 15rpx;
}
.index .attraction .info .wrapper .pic .location {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: space-evenly;
  -webkit-justify-content: space-evenly;
          justify-content: space-evenly;
  width: 120rpx;
  height: 40rpx;
  background-color: #24262F;
  border-radius: 30px;
  position: absolute;
  z-index: 99;
  top: 115rpx;
  left: 15rpx;
  text-align: right;
  color: #EEEEEE;
  font-size: 20rpx;
  line-height: 20rpx;
}
.index .attraction .info .wrapper .pic .location view {
  margin-top: 10rpx;
}
.index .attraction .info .wrapper .pic .location .region {
  margin-right: 3rpx;
}
.index .attraction .info .wrapper .pic .level {
  width: 50rpx;
  height: 40rpx;
  background-color: #FBD313;
  border-radius: 30px;
  position: absolute;
  z-index: 99;
  top: 20rpx;
  right: 20rpx;
  text-align: center;
  font-size: 24rpx;
  line-height: 24rpx;
}
.index .attraction .info .wrapper text {
  font-size: 28rpx;
  line-height: 60rpx;
  color: #EEEEEE;
  margin-left: 15rpx;
}
