swiper {
  width: 750rpx;
  height: 500rpx;
}
swiper image {
  height: 100%;
  width: 100%;
}
.wrapper {
  margin: 0 25rpx 0 25rpx;
}
.wrapper .title .h1 {
  font-size: 32rpx;
  line-height: 40rpx;
  color: #EEEEEE;
  margin-bottom: 20rpx;
}
.wrapper .title .label {
  margin-bottom: 35rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-top: 10rpx;
}
.wrapper .title .label .level {
  width: 95rpx;
  height: 36rpx;
  background-color: #2E2B21;
  color: #F0CA14;
  border-radius: 30px;
  text-align: center;
  font-size: 20rpx;
  line-height: 35rpx;
}
.wrapper .title .label view {
  width: 105rpx;
  height: 36rpx;
  background-color: #24262F;
  color: #88888E;
  border-radius: 30px;
  text-align: center;
  font-size: 20rpx;
  line-height: 35rpx;
  margin-left: 10rpx;
}
.wrapper .introduction {
  margin-top: 70rpx;
  margin-bottom: 20rpx;
}
.wrapper .introduction .h1 {
  font-size: 32rpx;
  line-height: 80rpx;
  color: #EEEEEE;
}
.wrapper .introduction .location {
  border-bottom: 1px solid rgba(238, 238, 238, 0.1);
}
.wrapper .introduction .location text {
  color: #88888E;
  line-height: 50rpx;
}
.wrapper .introduction .location .map {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  line-height: 50rpx;
}
.wrapper .introduction .location .map .map-left {
  color: #EEEEEE;
}
.wrapper .introduction .location .map .map-right {
  color: #EEEEEE;
  text-align: center;
  line-height: 20rpx;
}
.wrapper .introduction .location .map .map-right text {
  font-size: 23rpx;
}
.wrapper .introduction .time {
  margin-top: 20rpx;
  border-bottom: 1px solid rgba(238, 238, 238, 0.1);
}
.wrapper .introduction .time text {
  color: #88888E;
  line-height: 50rpx;
}
.wrapper .introduction .time view {
  line-height: 50rpx;
  color: #EEEEEE;
  margin-bottom: 18rpx;
}
.wrapper .introduction .phone {
  margin-top: 20rpx;
  border-bottom: 1px solid rgba(238, 238, 238, 0.1);
}
.wrapper .introduction .phone text {
  color: #88888E;
  line-height: 50rpx;
}
.wrapper .introduction .phone view {
  line-height: 50rpx;
  color: #EEEEEE;
  margin-bottom: 18rpx;
}
.wrapper .introduction .discount {
  margin-top: 20rpx;
  padding-bottom: 55rpx;
  border-bottom: 1px solid rgba(238, 238, 238, 0.1);
}
.wrapper .introduction .discount text {
  color: #88888E;
  line-height: 50rpx;
}
.wrapper .introduction .line {
  margin-top: 20rpx;
  padding-bottom: 55rpx;
  border-bottom: 1px solid rgba(238, 238, 238, 0.1);
}
.wrapper .introduction .line text {
  color: #88888E;
  line-height: 50rpx;
}
.wrapper .introduction .line .block {
  width: 750rpx;
  height: 100rpx;
}
.bottom {
  position: fixed;
  bottom: 0;
  width: 750rpx;
  height: 100rpx;
  background-color: #161722;
}
.bottom .view {
  width: 300rpx;
  height: 80rpx;
  border-radius: 50px;
  background-color: #FE2C55;
  margin: 10rpx auto;
}
.bottom .view view {
  font-size: 30rpx;
  line-height: 80rpx;
  color: #EEEEEE;
  margin: 0 90rpx;
}

