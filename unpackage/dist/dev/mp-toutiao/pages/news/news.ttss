.article .title {
  margin: 20rpx 25rpx 30rpx 25rpx;
}
.article .title .title-left {
  font-size: 32rpx;
  line-height: 32rpx;
  color: #EEEEEE;
}
.article .info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  width: 700rpx;
  height: 2000rpx;
  margin: 0 auto;
}
.article .info .info-left {
  width: 345rpx;
}
.article .info .info-left .wrapper {
  width: 345rpx;
  background-color: #24262F;
  border-radius: 15px;
  margin-bottom: 20rpx;
}
.article .info .info-left .wrapper image {
  width: 345rpx;
  height: 150rpx;
  margin-bottom: 10rpx;
  border-radius: 15px;
}
.article .info .info-left .wrapper text {
  display: inline-block;
  font-size: 26rpx;
  line-height: 50rpx;
  margin: 0 15rpx;
  color: #EEEEEE;
}
.article .info .info-left .wrapper .author image {
  margin: 30rpx 15rpx 0 15rpx;
  width: 24rpx;
  height: 24rpx;
}
.article .info .info-left .wrapper .author text {
  color: #EEEEEE;
  font-size: 24rpx;
  line-height: 70rpx;
}
.article .info .info-right {
  width: 345rpx;
}
.article .info .info-right .wrapper {
